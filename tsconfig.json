{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@UI": ["./src/core/ui/index.ts"],
      "@UI/Types": ["./types/core/ui/index.d.ts"],
      "@Packages": ["./src/core/packages/packages.ts"],
      "@Core/*": ["./src/core/*"],
      "@Core/Types": ["./types/core/index.d.ts"],
      "@CoreSymbols": ["./src/core/ioc/core.ioc.symbols.ts"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
